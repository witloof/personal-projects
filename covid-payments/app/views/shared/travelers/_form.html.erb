<%= form_with model: @traveler do |form| %>
    <%= form.fields_for :travels do |builder| %>
    <%= render "shared/travelers/travel_fields", f: builder %>
  <% end %>
  <%= form.fields_for :user do |builder| %>
    <%= render "shared/travelers/user_fields", f: builder %>
  <% end %>
  <div class="form-group">
    <%= form.label :residence_country_iso_code %>
    <%= form.select(:residence_country_iso_code, countries_options)%>
  </div>
  <div class="form-group">
    <%= form.label :passport_number %>
    <%= form.text_field :passport_number %>
  </div>
  <div class="form-group">
    <%= form.submit :send %>
  </div>
<% end %>