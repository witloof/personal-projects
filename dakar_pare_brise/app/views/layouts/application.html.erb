        window.dataLayer = window.dataLayer || [];
        function gtag(){dataLayer.push(arguments);}
        gtag('js', new Date());

        gtag('config', 'UA-110178477-1');
      </script>
      <% end %>
  </head>

  <body>
    <%= render partial: "layouts/header" %>
