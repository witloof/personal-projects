  <head>
    <title><%= page_title(app:"Dakar pare-brise") %></title>
    <%= csrf_meta_tags %>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="DAKAR PARE-BRISE est un Réseau de professionnels spécialisés dans la réparation et le remplacement de tous les vitrages automobiles. " />
    <meta name="keywords" content="pare-brise dakar sénégal vitrage pare brise senegal voiture " />
    <meta name="author" content="Dakar pare-brise" />

    <link rel="icon" type="image/png" href="<%= image_url('favicon-32x32.png') %>" sizes="32x32" />
    <link rel="icon" type="image/png" href="<%= image_url('favicon-16x16.png') %>" sizes="16x16" />
    <link href="https://fonts.googleapis.com/css?family=Roboto" rel="stylesheet">
